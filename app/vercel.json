{
    "version": 2,
    "builds": [
      {
        "src": "server/main.go",
        "use": "@vercel/go"
      },
      {
        "src": "app/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "app/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/main.go"
      },
      {
        "src": "/(.*)",
        "dest": "/app/$1"
      }
    ]
  }